<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/circle_radius">
            </corners>
            <solid android:color="@color/white"/>
            <size
                android:width="24dp"
                android:height="24dp">
            </size>
        </shape>
    </item>
    <item android:top="@dimen/btn_padding" android:bottom="@dimen/btn_padding"
        android:left="@dimen/btn_padding" android:right="@dimen/btn_padding">
        <shape
            android:shape="rectangle">
            <corners android:radius="@dimen/circle_radius">
            </corners>
            <solid android:color="@color/main_bg">
            </solid>

        </shape>

    </item>
</layer-list>